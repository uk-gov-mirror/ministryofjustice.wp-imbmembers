{
    "name": "roots",
    "version": "7.0.1",
    "author": "Ben Word <ben@benword.com>",
    "homepage": "http://roots.io",
    "description": "IMB Member web property",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/roots/roots.git"
    },
    "bugs": {
        "url": "https://github.com/roots/roots/issues"
    },
    "licenses": [
        {
            "type": "MIT",
            "url": "http://opensource.org/licenses/MIT"
        }
    ],
    "scripts": {
        "dev": "./node_modules/modernizr/bin/modernizr -c ./node_modules/modernizr/lib/config-all.json && NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && rm ./modernizr.js",
        "watch": "./node_modules/modernizr/bin/modernizr -c ./node_modules/modernizr/lib/config-all.json NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && rm ./modernizr.js",
        "hot": "./node_modules/modernizr/bin/modernizr -c ./node_modules/modernizr/lib/config-all.json NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js && rm ./modernizr.js",
        "production": "./node_modules/modernizr/bin/modernizr -c ./node_modules/modernizr/lib/config-all.json NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && rm ./modernizr.js"
    },
    "engines": {
        "node": ">= 5.12.0"
    },
    "dependencies": {
        "bootstrap": "^4.3.1",
        "jquery": "^3.3.1",
        "modernizr": "^3.7.1",
        "popper.js": "^1.14.7"
    },
    "devDependencies": {
        "acorn": "^6.1.1",
        "ajv": "^6.9.2",
        "browser-sync": "^2.26.3",
        "browser-sync-webpack-plugin": "^2.0.1",
        "cross-env": "^5.2.0",
        "details-element-polyfill": "^2.3.1",
        "laravel-mix": "^4.0.15",
        "marked": "^0.3.19",
        "node-sass": "^4.11.0",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.17.4",
        "sass-loader": "^7.1.0",
        "scrollpoints": "^0.4.0",
        "url-search-params-polyfill": "^4.0.0",
        "vue-template-compiler": "^2.6.8",
        "webpack": "^4.29.6"
    }
}
